{
    "type": "origins:multiple",
    "mount": {
        "type": "origins:action_on_being_used",
        "bientity_action": {
            "type": "origins:mount"
        },
        "bientity_condition": {
            "type": "origins:target_condition",
            "condition": {
                "type": "origins:passenger",
                "inverted": true,
                "comparison": ">=",
                "compare_to": 1
            }
        },
        "condition": {
            "type": "origins:power_active",
            "power": "ntorigins:centaur/rideable_toggle"
        }
    },
    "force_dismount": {
