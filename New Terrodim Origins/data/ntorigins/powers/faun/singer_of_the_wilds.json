{
    "type": "origins:multiple",
    "name": "\u00A7a\u00A7nSinger of the Wilds",
    "description": "The plants love you, and share their bounty with you.\n-double drops from flowers, mushrooms, leaves, berries, grass, melons, and cocoa-",
    "leaves": {
        "type": "origins:replace_loot_table",
        "replace": {
            "([a-z|0-9|\\-|_]).*:blocks\/(([a-z|0-9|\\-|_]).*)_leaves": "ntorigins:double_rolls"
        }
